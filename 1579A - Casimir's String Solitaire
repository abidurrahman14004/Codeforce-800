#include<bits/stdc++.h>
using namespace std;
int main(){
    int t; cin>>t;
    while(t--){
string str;
cin>>str;

int b=0;
for(int i=0;i<str.size();i++){

if(str[i]=='B')b++;


}

if(str.size()%2!=0)cout<<"NO"<<endl;

else if(str.size()/2==b){ cout<<"YES"<<endl;}
else cout<<"NO"<<endl;

    }
    return 0;
}
