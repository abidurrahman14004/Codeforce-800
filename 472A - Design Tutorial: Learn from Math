#include<bits/stdc++.h>
using namespace std;
bool judge(int n) {
if(n%2!=0 && n%3!=0 ){
    return false;
}
else return true;
}

int main(){
    int n; cin>>n;
     int a,b;
     a=n/2;
    b= n-a;
for(int i=0;i<n/2;i++){
if(judge(a)==true && judge(b)==true){
    break;
}
else a--;
     b++;

}
cout<<a<<" "<<b;

    return 0;
}
