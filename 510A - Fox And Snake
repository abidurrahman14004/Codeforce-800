#include<bits/stdc++.h>
using namespace std;

int main(){

int a,b;
cin>>a>>b;

for(int i=1;i<=a;i++){
    if(i%2!=0){
  for(int j=0;j<b;j++){
     cout<<"#";
  }
  printf("\n");
  
 }

    else if(i%4==0){
        cout<<"#";
    for(int j=0;j<b-1;j++){
        cout<<".";
    }
    printf("\n");}


    else if(i%2==0) {
    for(int j=0;j<b-1;j++){
    cout<<".";
    }
    cout<<"#";
    printf("\n");
    }

}
    
  return 0;
}
