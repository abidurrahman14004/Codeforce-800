#include <bits/stdc++.h>
using namespace std;

int main(){
 vector<pair<int,int>> v;
 int n,count=0;
 cin>>n;
 for(int i=0; i<n; i++){
    int x,y;
    cin>>x>>y;
    v.push_back({x,y});
 }
 for(int i=0; i<n; i++){
   for(int j=0; j<n; j++){
    if(v[i].second==v[j].first) count++;
    
   }  
    }
 cout<<count<<endl;


    return 0;
}
